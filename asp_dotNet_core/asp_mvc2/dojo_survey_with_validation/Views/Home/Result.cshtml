
@{
    ViewData["Title"] = "Home Page";
}

@model User

<div class="container px-4">
        <div class="row gx-5">
            <div class="col">
                <div class="p-3 border bg-light">

                    
                    <div class="mb-3">
                        <label for="name" class="form-label">Your Name:  @Model.userName<h4></h4></label>
                    </div>
                    <div class="mb-3">
                        <label for="dojoLocation" class="form-label">Dojo Location:  @Model.userDojoLocation<h4></h4></label>
                    </div>
                    <div class="mb-3">
                        <label for="favoriteLanguage" class="form-label">Favorite Language:  @Model.userFavoriteLanguage<h4></h4></label>

                    </div>
                    <div class="mb-3">
                        <label for="comment" class="form-label">Comment:  @Model.userComment<h4></h4></label>

                    </div>

                </div>
            </div>
        </div>
    </div>